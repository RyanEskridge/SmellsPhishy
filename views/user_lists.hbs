<h1 class="mx-2">{{title}}</h1>
<p class="mx-2">{{description}}</p>
<div class="container-fluid mt-4">
	<div class="row">
		{{#each lists}}
		<div class="card mx-2" style="width: 18rem;">
			<div class="card-body">
				<h5 class="card-title">{{name}}</h5>
				<h6 class="card-subtitle mb-2 text-body-secondary"><small>{{date}}</small></h6>
				<p class="card-text my-2">{{description}}</p>
				<button type="button" class="btn btn-secondary my-2">
					Recipients
					<span class="badge text-bg-dark">{{recipients}}</span>
				</button>
				<button type="button" class="btn btn-dark" style="">View</button>
			</div>
		</div>
		{{/each}}
	</div>
</div>
